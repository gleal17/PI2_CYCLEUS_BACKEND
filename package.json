{
  "name": "cycleus_backend",
  "version": "1.0.0",
  "description": " backend projeto cycleus",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "nodemon main.js",
    "dev": "nodemon main.js",
    "migration:run": "typeorm migration:run",
    "migration:generate": "typeorm migration:generate",
    "migration:revert": "typeorm migration:revert",
    "clean": "rimraf dist",
    "typeorm": "ts-node-dev node_modules/.bin/typeorm",
    "db:seed": "ts-node-dev node_modules/typeorm-seeding/dist/cli.js seed",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "paths": {
    "@entities/*": [
      "src/entities/*"
    ],
    "@database/*": [
      "src/database/*"
    ],
    "@middlewares/*": [
      "src/middlewares/*"
    ],
    "@controllers/*": [
      "src/controllers/*"
    ],
    "@migrations/*": [
      "src/database/migrations/*"
    ],
    "@factories/*": [
      "src/database/factories/*"
    ],
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.1",
    "mongodb": "^5.6.0",
    "nodemon": "^2.0.22",
    "pg": "^8.11.1",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.32.1"
  },
  "devDependencies": {
    "@types/node": "^20.4.1",
    "typeorm": "^0.3.17"
  }
}
