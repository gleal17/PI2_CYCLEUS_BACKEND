{
  "name": "cycleus_backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "ts-node src/main.js",
    "dev": "ts-node-dev --max_old_space_size=512 --respawn --transpile-only src/main.ts",
    "clean": "rimraf dist",
    "build": "npm run clean && tsc -p tsconfig.json",
    "typeorm": "ts-node-dev node_modules/.bin/typeorm",
    "db:seed": "ts-node-dev node_modules/typeorm-seeding/dist/cli.js seed",
    "migration:generate": "npm run typeorm -- migration:generate src/db/migrations/default -d src/db/config.ts",
    "migration:run": "npm run typeorm -- migration:run -d src/db/config.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d src/db/config.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "paths": {
    "@entities/*": [
      "src/entities/*"
    ],
    "@database/*": [
      "src/database/*"
    ],
    "@middlewares/*": [
      "src/middlewares/*"
    ],
    "@controllers/*": [
      "src/controllers/*"
    ],
    "@migrations/*": [
      "src/database/migrations/*"
    ],
    "@factories/*": [
      "src/database/factories/*"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.1",
    "mongodb": "^5.6.0",
    "nodemon": "^2.0.22",
    "pg": "^8.11.1",
    "pg-hstore": "^2.3.4",
    "postgres": "^3.3.5",
    "prisma": "^5.0.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.32.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.4.1",
    "typeorm": "^0.3.17",
    "typescript": "^5.1.6"
  }
}
